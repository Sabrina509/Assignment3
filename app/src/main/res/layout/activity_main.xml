<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/rollnotext"
        android:layout_width="173dp"
        android:layout_height="52dp"
        android:layout_marginStart="100dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="62dp"
        android:layout_marginEnd="84dp"
        android:layout_marginRight="84dp"
        android:layout_marginBottom="390dp"
        android:text="BCSF18A509"
        android:textColor="#0B0A0A"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/repositarybtn"
        app:layout_constraintEnd_toStartOf="@+id/nametext"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth="wrap_content"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/nametext"
        android:layout_width="134dp"
        android:layout_height="39dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="100dp"
        android:layout_marginRight="100dp"
        android:layout_marginBottom="395dp"
        android:text="Sabrina Asim"
        android:textColor="#101010"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/assignmentbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rollnotext"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth="wrap_content"
        tools:visibility="visible" />

    <Button
        android:id="@+id/repositarybtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="440dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:text="Repositary Btn"
        app:layout_constraintEnd_toStartOf="@+id/assignmentbtn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rollnotext" />

    <Button
        android:id="@+id/assignmentbtn"
        android:layout_width="155dp"
        android:layout_height="54dp"
        android:layout_marginTop="448dp"
        android:layout_marginEnd="25dp"
        android:layout_marginRight="25dp"
        android:text="Assignment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/repositarybtn"
        app:layout_constraintTop_toBottomOf="@+id/nametext" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="292dp"
        android:layout_height="301dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/repositary" />

</androidx.constraintlayout.widget.ConstraintLayout>